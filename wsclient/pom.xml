            <exclude>**/*IntegrationTest_54.java</exclude>
            <exclude>**/WSFileIOClientTest.java</exclude>
            <exclude>**/WUQueryTest.java</exclude>
          </excludes>
        </configuration>
      </plugin>
   </plugins>
